    <fragment
        android:id="@+id/registrarServicioFragment"
        android:name="cr.una.buildify.servicios.RegistrarServicioFragment"
        android:label="fragment_registrar_servicio"
        tools:layout="@layout/fragment_registrar_servicio" />
    <fragment
        android:id="@+id/calificacionesTrabajador"
        android:name="cr.una.buildify.servicios.CalificacionesFragment"
        android:label="fragment_calificaciones"
        tools:layout="@layout/fragment_calificaciones" />
    <fragment
        android:id="@+id/buscarServicioFragment"
        android:name="cr.una.buildify.servicios.buscarServicioFragment"
        android:label="fragment_buscarServicio"
